doctype html
html
	head
		meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
		meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0')

		link(href='css/style.css', rel='stylesheet')
		link(rel='stylesheet', type='text/css', href='semantic/dist/semantic.min.css')
		script(src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.0.0-beta1/jquery.min.js")
		script(src='semantic/dist/semantic.min.js')
		script(src='js/script.js')
	
	body

		// Navbar Menu
		.navbar.navone.ui.top.fixed.menu.transition.inverted.secondary.pointing
			.ui.container
				a.item.img(href='#home')
					img(src="img/logo.png")
				.right.menu
					a#navicon.closed.btn.toc.item
						svg#ht(viewbox='0 0 15 15')
							rect(x='3', y='5', width='9', height='1')
						svg#hm(viewbox='0 0 15 15')
							rect(x='3', y='7', width='9', height='1')
						svg#hb(viewbox='0 0 15 15')
							rect(x='3', y='9', width='9', height='1')
					a.ui.item.home.active(href="#home") Home
					a.ui.item.one(href="#one") One
					a.ui.item.two(href="#two") Two
					a.ui.item.three(href="#three") Three
					a.ui.item.four(href="#four") Four
					a.ui.item.five(href="#five") Five
					
		// Sidebar Menu
		.navbar.ui.vertical.inverted.sidebar.menu.right
			a.ui.item.home.active(href="#home") Home
			a.ui.item.one(href="#one") One
			a.ui.item.two(href="#two") Two
			a.ui.item.three(href="#three") Three
			a.ui.item.four(href="#four") Four
			a.ui.item.five(href="#five") Five

